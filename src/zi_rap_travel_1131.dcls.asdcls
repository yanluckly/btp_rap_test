@EndUserText.label: 'Access control for ZI_RAP_TRAVEL_1131'
@MappingRole: true
define role ZI_RAP_TRAVEL_1131 {
    grant
        select
            on
                ZI_RAP_TRAVEL_1131
                    where
                        ( TravelStatus )
                      = aspect pfcg_auth ( ZOSTAT1131, ZOSTAT1131, actvt = '03' )
                        and
                        CurrencyCode = 'EUR';
}